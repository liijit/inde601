  "scripts": {
    "lint": "eslint \"src\" --ext .ts --fix",
    "prettier-css": "cd frontend/css && prettier --find-config-path -w *.css",
    "prettier-html": "cd frontend && prettier --find-config-path -w *.html",
    "prettier-err": "prettier --find-config-path -w ./frontend/css/*.css",
    "dev": "tsc && nodemon -r dotenv/config src/app.ts && nodemon --watch 'src/**'",
    "html": "browser-sync start --server --files '*.html, css/*.css'",
    "dev-typescript-w": "tsnd --respawn src/app.ts --watch src/*.ts",
    "prod": "tsc && node ts-pile/app.js"
